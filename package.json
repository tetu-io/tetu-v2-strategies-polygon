{
  "name": "tetu-v2-strategies-polygon",
  "version": "2.0.0",
  "description": "Tetu V2 strategies on Polygon",
  "main": "hardhat.config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "npx hardhat clean",
    "compile": "npx hardhat compile",
    "test": "npm run build && npx hardhat test",
    "coverage": "npm run build && npx cross-env NODE_OPTIONS=\"--max_old_space_size=8192\" hardhat coverage --temp artifacts --max-memory 4096",
    "slither": "npm run build && slither .",
    "lint": "tslint -p tsconfig.json",
    "lint-fix": "tslint --fix -p tsconfig.json",
    "update-tetu-contracts-v2": "npm update @tetu_io/tetu-contracts-v2",
    "update-block": "ts-node scripts/utils/UpdateBlock.ts"
  },
  "keywords": [
    "solidity",
    "ethereum",
    "polygon",
    "bsc",
    "smart",
    "contracts",
    "tetu"
  ],
  "author": "TETU team <admin@tetu.io>",
  "license": "MIT",
  "homepage": "https://tetu.io/",
  "dependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@nomiclabs/hardhat-etherscan": "^3.1.6",
    "@nomiclabs/hardhat-solhint": "^2.0.1",
    "@tetu_io/tetu-contracts-v2": "git+https://github.com/tetu-io/tetu-contracts-v2.git#master",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-string": "^1.4.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.1",
    "axios": "^1.3.4",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "csv-stringify": "^6.2.4",
    "dotenv": "^16.0.3",
    "ethereumjs-tx": "^2.1.2",
    "ethers": "^5.7.2",
    "hardhat": "^2.12.7",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-contract-sizer": "^2.8.0",
    "hardhat-deploy": "^0.11.23",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-tracer": "^1.3.0",
    "hardhat-typechain": "^0.3.5",
    "mocha": "^10.2.0",
    "solidity-coverage": "^0.8.2",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslog": "^3.2.2",
    "typechain": "^8.1.1",
    "typescript": "^4.9.5",
    "yargs": "^17.7.1"
  },
  "engines": {
    "node": "^14.18.1"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "web3": "^1.8.2"
  }
}
